                      "hsCode": {
                        "type": "string"
                      },
                      "hsDescription": {
                        "type": "string"
                      },
                      "buyer": {
                        "type": "string"
                      },
                      "buyerCompanyCode": {
                        "type": "string"
                      },
                      "volume": {
                        "type": "number"
                      },
                      "value": {
                        "type": "number"
                      }
                    }
                  }
                }
              },
