{
  "name": "visibility",
  "version": "0.1.0",
  "private": true,
  "config": {
    "mongodbMemoryServer": {
      "debug": "1"
    }
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.ts,.tsx && prettier --write .",
    "test:server": "export NODE_ENV=test && jest -c server/__test__/jest.server.config.js --runInBand",
    "test:client": "export NODE_ENV=test && jest -c client/jest.client.config.js",
    "test": "export NODE_ENV=test && jest --runInBand",
    "test:coverage": "export NODE_ENV=test && jest --updateSnapshot --coverage --runInBand",
    "test:ci": "export NODE_ENV=ci && jest --runInBand",
    "test:coverage:ci": "export NODE_ENV=ci && jest --updateSnapshot --coverage --runInBand",
    "migrate:local": "node server/migrate.js -m ./server/migrations --env local",
    "migrate:dev": "node server/migrate.js -m ./server/migrations --env dev",
    "migrate:preview": "node server/migrate.js -m ./server/migrations --env preview",
    "migrate:production": "node server/migrate.js -m ./server/migrations --env production",
    "migrate:ci": "node server/migrate.js -m ./server/migrations --env ci",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "generate-iconfont": "icon-font-generator client/resources/icons/*.svg -o client/resources/styles/iconfons && mv client/resources/styles/iconfons/icons.css client/resources/styles/iconfons/icons.scss"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@antv/g6": "^4.1.13",
    "@mapbox/mapbox-gl-geocoder": "^4.7.0",
    "@material-ui/core": "^4.11.3",
    "@material-ui/data-grid": "^4.0.0-alpha.21",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@types/luxon": "^1.26.2",
    "chart.js": "^2.9.4",
    "classnames": "^2.2.6",
    "db-migrate": "^0.11.12",
    "db-migrate-mongodb": "^1.5.0",
    "lodash": "^4.17.21",
    "luxon": "^1.26.0",
    "mapbox-gl": "^2.1.1",
    "moment": "^2.29.1",
    "mongodb": "^3.6.4",
    "mongodb-memory-server": "7.0.0-beta.22",
    "mongoose": "^5.11.17",
    "next": "10.0.7",
    "next-connect": "^0.10.0",
    "numeral": "^2.0.6",
    "react": "17.0.1",
    "react-chartjs-2": "^2.11.1",
    "react-dom": "17.0.1",
    "react-mapbox-gl": "^5.1.1",
    "react-window": "^1.8.6",
    "storybook": "^6.2.8",
    "swagger-ui-react": "^3.47.1",
    "swr": "^0.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@storybook/addon-actions": "^6.2.8",
    "@storybook/addon-essentials": "^6.2.8",
    "@storybook/addon-links": "^6.2.8",
    "@storybook/react": "^6.2.8",
    "@testing-library/dom": "^7.29.6",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/chartjs": "^0.0.31",
    "@types/classnames": "^2.2.11",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/mapbox__mapbox-gl-geocoder": "^4.7.0",
    "@types/node": "^14.14.31",
    "@types/numeral": "^2.0.0",
    "@types/react": "^17.0.2",
    "@types/react-window": "^1.8.2",
    "@types/swagger-ui-react": "^3.35.0",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-preset-react-app": "^10.0.0",
    "css-loader": "^5.2.4",
    "db-migrate-plugin-typescript": "^2.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "4.2.5",
    "icon-font-generator": "^2.1.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-canvas-mock": "^2.3.1",
    "jest-dom": "^4.0.0",
    "jest-enzyme": "^7.1.2",
    "lint-staged": "^10.5.4",
    "node-mocks-http": "^1.10.1",
    "prepush-if-changed": "^1.0.8",
    "prettier": "^2.2.1",
    "sass": "^1.32.11",
    "sass-loader": "10.1.1",
    "style-loader": "^2.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5",
    "winston": "^3.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && tsc",
      "pre-push": "prepush-if-changed",
      "commit-msg": "node commit-message-check.js"
    }
  },
  "prepush-if-changed": {
    "(shared|client)/**": "yarn test:client --coverage",
    "(shared|server)/**": "yarn test:server --coverage"
  },
  "lint-staged": {
    "*{.ts,.js,.tsx}": [
      "eslint --fix . --ext .js,.ts,.tsx",
      "prettier --write .",
      "git add"
    ]
  }
}
