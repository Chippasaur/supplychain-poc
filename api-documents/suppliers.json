    "/suppliers/{id}/buyers": {
      "get": {
        "tags": ["suppliers"],
        "summary": "get supplier's buyers by supplier id",
        "produces": ["application/json"],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "ID of supplier",
            "required": true,
            "type": "string",
            "format": "int64"
          },
          {
            "name": "source",
            "in": "query",
            "description": "contributors or shipments",
            "required": true,
            "type": "string",
            "format": "int64"
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "id": {
                    "type": "string"
                  },
                  "buyers": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  }
                }
